{
  "name": "yagnostic-api",
  "version": "1.0.0",
  "description": "Yagnostic main api service",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf node_modules dist",
    "build": "webpack --mode production",
    "start-prod": "DEBUG=yagnostic-api* NODE_ENV=production node dist/app.js",
    "start-dev": "DEBUG=yagnostic-api* NODE_ENV=development nodemon index.js",
    "test": "NODE_ENV=test jest --forceExit --runInBand",
    "migrate": "knex migrate:latest",
    "migrate-down": "knex migrate:down"
  },
  "author": "Yagnostic",
  "license": "ISC",
  "keywords": [
    "express"
  ],
  "engines": {
    "node": ">= 10.21.0",
    "yarn": ">= 1.22.4"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.21.5",
    "nodemailer": "^6.6.0",
    "pg": "^8.3.3",
    "swagger-ui-express": "^4.1.4",
    "web3": "^1.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "jasmine-expect": "^4.0.3",
    "jest": "^26.0.1",
    "jest-environment-node": "^25.5.0",
    "jest-extended": "^0.11.5",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2",
    "webpackbar": "^4.0.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jasmine-expect",
      "jest-extended",
      "./jest.setup.js"
    ],
    "testEnvironment": "./jest.env.js"
  },
  "peerDependencies": {
    "eslint": "*",
    "nodemon": "*"
  }
}
